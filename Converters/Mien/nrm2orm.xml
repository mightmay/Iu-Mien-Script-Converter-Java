</classes>

<mappingTable id="cons">
<columns>
	<component r="c"/>
	<component r="oc"/>
</columns>
<maps>
	<m><c r="c"/><c r="oc"/></m>
	<m><c r="c" class="highLowCons"/><c r="oc" class="highLowCons"/></m>
	<m><c r="c" class="midCons"/><c r="oc" class="midCons"/></m>
</maps>
</mappingTable>

<mappingTable id="final">
	<columns>
		<component r="f"/>
		<component r="of"/>
	</columns>
	<maps>
		<m><c r="f"/><c r="of"/></m>
		<m><c r="f" class="anyFinal"/><c r="of" class="anyFinal"/></m>
	</maps>
</mappingTable>

<mappingTable id="tone">
<columns>
<component r="t"/>
<component r="ot"/>
</columns>
<maps>
	<m><c r="t"/><c r="ot"/></m>
	<m><c r="t" class="anyTone"/><c r="ot" class="anyTone"/></m>
</maps>
</mappingTable>

