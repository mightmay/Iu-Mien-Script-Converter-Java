<mappingTable id="cons-tone">
<columns>
<component r="c"/>
<component r="t"/>
<component r="f"/>
<component r="tc"/>
<component r="tt"/>
<component r="tf"/>
</columns>
<maps>
<!-- template 
<m><c r="c"></c> <c r="t"></c> <c r="l-c"></c> <c r="l-t"></c></m>
<m>
  <c r="c"></c> <c r="t"></c><c r="f"></c>
  <c r="tc"/><c r="tt"/><c r="tf"/>
</m>
-->
<!-- Low Consonants -->
<m><!-- unmarked tone -->
<!-- not sure about the final here! it is ambiguous in reverse -->
<c r="c" class="lowCons"/> <c r="t"/><c r="f" class="mnngFinal"/> 
<c r="tc" class="lowCons"/> <c r="tt"/><c r="tf" class="mnngFinal"/>
</m>
<m><!-- unmarked tone -->
	<!-- This is ambiguous in reverse, but should be resolved by 
		 vowel-final table -->
	<c r="c" class="lowCons"/> <c r="t"/><c r="f"/> 
	<c r="lc" class="lowCons"/> <c r="tt"/><c r="tf"/>
</m>
<m><!-- v tone, glottal -->
	<c r="c" class="lowCons"/> <c r="t">v</c><c r="f"/>
	<c r="lc" class="lowCons"/> <c r="tt"/><c r="tf"/>
</m>
<m><!-- v tone, stop -->
<c r="c" class="lowCons"/> <c r="t">v</c><c r="f" class="kptFinal"/> 
<c r="tc" class="lowCons"/> <c r="tt"/><c r="tf" class="kptFinal"/>
</m>
<m><!-- v tone, other -->
<c r="c" class="lowCons"/> <c r="t">v</c><c r="f" class="mnngFinal"/> 
<c r="tc" class="lowCons"/> <c r="tt" hex="0e49"/><c r="tf" class="mnngFinal"/>
</m>
<m><!-- v tone, no final -->
<c r="c" class="lowCons"/> <c r="t">v</c><c r="f" /> 
<c r="tc" class="lowCons"/> <c r="tt" hex="0e49"/><c r="tf" />
</m>
<m><!-- hz tone -->
<c r="c" class="lowCons"/> <c r="t" class="hzTone"/><c r="f" class="anyFinal"/>
<c r="tc" class="lowCons"/> <c r="tt" class="hzTone"/><c r="tf" class="anyFinal"/>
</m>

<!-- Mid Consonants -->
<m><!-- unmarked tone -->
<!-- not sure about the final here! this stops it being ambiguous in reverse -->
<c r="c" class="midCons"/> <c r="t"/><c r="f" class="mnngFinal"/> 
<c r="tc" class="midCons"/> <c r="tt"/><c r="tf" class="mnngFinal"/>
</m>
<m><!-- unmarked tone -->
	<c r="c" class="midCons"/> <c r="t"/><c r="f"/> 
	<c r="lc" class="midCons"/> <c r="tt" /><c r="tf"/>
</m>
<m><!-- c tone, glottal -->
	<c r="c" class="midCons"/> <c r="t">c</c><c r="f"/> 
	<c r="tc" class="midCons"/> <c r="tt"/><c r="tf"/>
</m>
<m><!-- c tone, stop -->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f" class="kptFinal"/> 
<c r="tc" class="midCons"/> <c r="tt"/><c r="tf" class="kptFinal"/>
</m>
<m><!-- c tone, other -->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f" class="mnngFinal"/> 
<c r="tc" class="midCons"/> <c r="tt" hex="0e48"/><c r="tf" class="mnngFinal"/>
</m>
<m><!-- c tone, other -->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f"/> 
<c r="tc" class="midCons"/> <c r="tt" hex="0e48"/><c r="tf"/>
</m>
<m><!-- hz tone -->
<c r="c" class="midCons"/> <c r="t" class="hvxzMidTone"/><c r="f" class="anyFinal"/>
<c r="tc" class="midCons"/> <c r="tt" class="hvxzMidTone"/><c r="tf" class="anyFinal"/>
</m>

<!-- High Consonants -->
<m><!-- unmarked tone -->
	<c r="c" class="highCons"/> <c r="t">x</c><c r="f" class="mnngFinal"/> 
	<c r="tc" class="highCons"/> <c r="tt"/><c r="tf" class="mnngFinal"/>
</m>
<m><!-- unmarked tone -->
	<c r="c" class="highCons"/> <c r="t">x</c><c r="f"/> 
	<c r="tc" class="highCons"/> <c r="tt" /><c r="tf"/>
</m>
<m><!-- c tone, glottal -->
	<c r="c" class="highCons"/> <c r="t">c</c><c r="f"/> 
	<c r="lc" class="highCons"/> <c r="tt"/><c r="tf"/>
</m>
<m><!-- c tone, stop -->
<c r="c" class="highCons"/> <c r="t">c</c><c r="f" class="kptFinal"/> 
<c r="tc" class="highCons"/> <c r="tt"/><c r="tf" class="kptFinal"/>
</m>
<m><!-- c tone, other -->
<c r="c" class="highCons"/> <c r="t">c</c><c r="f" class="mnngFinal"/> 
<c r="tc" class="highCons"/> <c r="tt" hex="0e48"/><c r="tf" class="mnngFinal"/>
</m>
<m><!-- c tone, other -->
<c r="c" class="highCons"/> <c r="t">c</c><c r="f" /> 
<c r="tc" class="highCons"/> <c r="tt" hex="0e48"/><c r="tf" />
</m>
