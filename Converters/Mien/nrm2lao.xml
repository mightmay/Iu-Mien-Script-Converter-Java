<mappingTable id="cons-tone">
<columns>
<component r="c"/>
<component r="t"/>
<component r="f"/>
<component r="lc"/>
<component r="lt"/>
<component r="lf"/>
</columns>
<maps>
<!-- template 
<m><c r="c"></c> <c r="t"></c> <c r="l-c"></c> <c r="l-t"></c></m>
<m>
  <c r="c"></c> <c r="t"></c><c r="f"></c>
  <c r="lc" /><c r="lt" /><c r="lf" />
</m>
-->
<!-- Low Consonants -->
<m><!-- unmarked tone -->
<!-- not sure about the final here! it is ambiguous in reverse -->
<c r="c" class="lowCons"/> <c r="t"/><c r="f" class="mnngFinal"/> 
<c r="lc" class="lowCons"/> <c r="lt"/><c r="lf" class="mnngFinal"/>
</m>
<m><!-- unmarked tone -->
  <!-- not sure about the final here! it is ambiguous in reverse -->
  <c r="c" class="lowCons"/> <c r="t"/><c r="f"/> 
  <c r="lc" class="lowCons"/> <c r="lt"/><c r="lf"/>
</m>
<m><!-- v tone, stop -->
	<c r="c" class="lowCons"/> <c r="t">v</c><c r="f" class="kptFinal"/> 
	<c r="lc" class="lowCons"/> <c r="lt"/><c r="lf" class="kptFinal"/>
</m>
<m><!-- v tone, glottal -->
<c r="c" class="lowCons"/> <c r="t">v</c><c r="f"/>
<c r="lc" class="lowCons"/> <c r="lt"/><c r="lf"/>
</m>
<m><!-- v tone, other -->
<c r="c" class="lowCons"/> <c r="t">v</c><c r="f" class="mnngFinal"/> 
<c r="lc" class="lowCons"/> <c r="lt" hex="0ec9"/><c r="lf" class="mnngFinal"/>
</m>
<m><!-- v tone, no final -->
<c r="c" class="lowCons"/> <c r="t">v</c><c r="f" /> 
<c r="lc" class="lowCons"/> <c r="lt" hex="0ec9"/><c r="lf" />
</m>
<m><!-- hz tone -->
<c r="c" class="lowCons"/> <c r="t" class="hzTone"/><c r="f" class="anyFinal"/>
<c r="lc" class="lowCons"/> <c r="lt" class="hzTone"/><c r="lf" class="anyFinal"/>
</m>

<!-- Mid Consonants -->
<m><!-- unmarked tone -->
<!-- not sure about the final here! this stops it being ambiguous in reverse -->
<c r="c" class="midCons"/> <c r="t"/><c r="f" class="mnngFinal"/> 
<c r="lc" class="midCons"/> <c r="lt" /><c r="lf" class="mnngFinal"/>
</m>
<m><!-- unmarked tone -->
<c r="c" class="midCons"/> <c r="t"/><c r="f"/> 
<c r="lc" class="midCons"/> <c r="lt" /><c r="lf"/>
</m>
<m><!-- c tone, glottal -->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f" /> 
<c r="lc" class="midCons"/> <c r="lt"/><c r="lf" />
</m>
<m><!-- c tone, stop-->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f" class="kptFinal"/> 
<c r="lc" class="midCons"/> <c r="lt"/><c r="lf" class="kptFinal"/>
</m>
<m><!-- c tone, other -->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f" class="mnngFinal"/> 
<c r="lc" class="midCons"/> <c r="lt" hex="0ec8"/><c r="lf" class="mnngFinal"/>
</m>
<m><!-- c tone, other -->
<c r="c" class="midCons"/> <c r="t">c</c><c r="f"/> 
<c r="lc" class="midCons"/> <c r="lt" hex="0ec8"/><c r="lf"/>
</m>
<m><!-- hz tone -->
<c r="c" class="midCons"/> <c r="t" class="hvxzMidTone"/><c r="f" class="anyFinal"/>
<c r="lc" class="midCons"/> <c r="lt" class="hvxzMidTone"/><c r="lf" class="anyFinal"/>
</m>

<!-- High Consonants -->
<m><!-- unmarked tone -->
  <c r="c" class="highCons"/> <c r="t">x</c><c r="f" class="mnngFinal"/> 
  <c r="lc" class="highCons"/> <c r="lt" /><c r="lf" class="mnngFinal"/>
