<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xml" href="SyllableConverter.xsl"?>
<syllableConverter xmlns="http://www.thanlwinsoft.org/schemas/SyllableParser">
<script side="left" ignoreCase="true">
<name>Mien NRM</name>
<!-- Mien NRM -->
<cluster side="left" id="syllable">
  <component id="p" min="0" priority="20">pre-syllable</component>
  <component id="c" min="0" priority="70">consonant</component>
  <component id="sv" min="0" priority="10">semi-vowel </component>
  <component id="v" min="0" priority="80">vowel</component>
  <component id="f" min="0" priority="50">final</component>
  <component id="t" min="0" priority="40">tone</component>
</cluster>
</script>

<script side="right">
<name>Mien Lao</name>
<!-- Mien Lao -->
<cluster side="right" id="syllable">
  <component id="lp" min="0" priority="30"/><!--pre-syllable -->
  <component id="lvp" min="0" priority="50"/><!--pre-vowel -->
  <component id="lc" min="0" priority="50"/><!--consonant -->
  <component id="ls" min="0" priority="10"/><!--semi-vowel -->
  <component id="lvu" min="0" priority="20"/><!--vowel-above -->
  <component id="lvd" min="0" priority="50"/><!--vowel-below -->
  <component id="lt" min="0" priority="50"/><!--tone -->
  <component id="lva" min="0" priority="70"/><!--vowel-after -->
  <component id="lf" min="0"/><!--final -->
</cluster>
</script>

<checks>
  <checker class="DocCharConvert.Converter.syllable.ExceptionList">
    <!--<arg>mienNrm2LaoExceptions.txt</arg>-->
    <!-- files must have path specified relative to this xml file -->
    <arg type="file">mienNrm2LaoNrmExceptions.txt</arg>
    <arg type="file">mienNrm2LaoLaoUExceptions.txt</arg>
  </checker>
  <checker class="DocCharConvert.Converter.syllable.MienSyllableSeparators">
  </checker>
  <checker class="DocCharConvert.Converter.syllable.CapitalizeSentences">
  </checker>
</checks>

<repeat>
